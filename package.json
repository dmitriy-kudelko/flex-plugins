{
  "name": "flex-plugins",
  "packageManager": "yarn@3.6.1",
  "engines": {
    "node": "16"
  },
  "workspaces": [
    "packages/*",
    "plugins/*"
  ],
  "devDependencies": {
    "prettier": "3.0.0"
